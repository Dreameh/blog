{
  "name": "dreameh-orgs",
  "description": "Gatsby starter website with org-mode",
  "version": "0.0.1",
  "author": "Xiaoxing Hu <dawnstar.hu@gmail.com>, Dreameh",
  "dependencies": {
    "bulma": "^0.7.5",
    "gatsby": "^2.14.0",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-orga": "^1.5.2",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-transformer-remark": "^2.6.28",
    "prismjs": "^1.17.1",
    "gatsby-remark-prismjs": "^3.3.10",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-icons": "^3.7.0",
    "node-sass": "^4.9.2"
  },
  "keywords": [
    "gatsby",
    "org-mode",
    "orga"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "rm -rf .cache && gatsby develop",
